<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="AvenirNextTextView">
        <attr name="AvenirNext_Regular" format="string" />
    </declare-styleable>
    <declare-styleable name="AvenirNextULTextView">
        <attr name="AvenirNext_UltraLight" format="string" />
    </declare-styleable>
    <declare-styleable name="RobotoTextView">
        <attr name="Roboto_Regular" format="string" />
    </declare-styleable>

    <!-- Defines the custom XML attributes supported for a HorizontalListView -->
    <declare-styleable name="HorizontalListView">
        <attr name="dividerWidth" format="dimension" />
        <attr name="android:divider" />
        <attr name="android:requiresFadingEdge" />
        <attr name="android:fadingEdgeLength" />
    </declare-styleable>

</resources>